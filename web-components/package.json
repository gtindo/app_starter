  "scripts": {
    "build": "npm run build:css && npm run build:components && npm run build:libs",
    "watch:css": "tailwindcss -i src/styles/base.css -o dist/css/base.css --watch",
    "build:css": "tailwindcss -i src/styles/base.css -o dist/css/base.css",
    "build:components": "esbuild src/**/*.tsx --target=chrome58,firefox57,safari11,edge18 --outdir=dist/js/components",
    "build:libs": "esbuild src/libs/*.ts --bundle --minify --target=chrome58,firefox57,safari11,edge18 --outdir=dist/js/libs",
    "dev": "storybook dev -p 6006",
    "build:doc": "storybook build"
  },
